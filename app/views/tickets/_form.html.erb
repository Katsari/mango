<%= render_error_messages(f.error_messages) %>
<label for="ticket_type_id" class="small">Tipo de ticket:</label>
<div class="field"><%= f.collection_select :ticket_type_id, @ticket_types, :id, :description, {:include_blank => true}, :class=>'chosen-select chzn-field-normal' %><span>*</span></div>
<label for="driver_id" class="small">Chofer:</label>
<div class="field"><%= f.collection_select :driver_id, @drivers, :id, :name, {:include_blank => true}, :class=>'chosen-select chzn-field-normal' %><span>*</span></div>
<label for="truck_id" class="small">Cami√≥n:</label>
<div class="field"><%= f.collection_select :truck_id, @trucks, :id, :license_plate, {:include_blank => true}, :class=>'chosen-select chzn-field-normal' %><span>*</span></div>
<label for="incoming_date" class="small">Fecha de entrada:</label>
<div class="field"><%= f.datetime_select :incoming_date, :include_seconds => true, :default => DateTime.now, :size=>'10' %> <span>*</span></div>
<label for="incoming_weight" class="small">Peso de entrada:</label>
<div class="field"><%= f.text_field :incoming_weight, :size=>'20' %><span>*</span></div>
<label for="provider_weight" class="small">Peso SADA:</label>
<div class="field"><%= f.text_field :provider_weight, :size=>'20' %><span>*</span></div>
<label for="provider_document_number" class="small">N# documento SADA:</label>
<div class="field"><%= f.text_field :provider_document_number, :size=>'20' %><span>*</span></div>
<label for="comment" class="small">Comentario;</label>
<div class="field"><%= f.text_field :comment, :size=>'60' %></div>
<div class="clearfix"></div>
<br/><span class="required">* Campos obligatorios</span>
